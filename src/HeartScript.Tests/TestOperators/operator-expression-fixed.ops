#Pass FixedExpressions Delimiter Terminator
'{' -> none none '{' expr ',' expr ',' expr '}'

#Pass FixedExpressions NoDelimiter Terminator
'[' -> none none '[' expr expr expr ']'

#Pass FixedExpressions Delimiter NoTerminator
'?' -> none none '?' expr ':' expr ':' expr

#Pass FixedExpressions NoDelimiter NoTerminator
'|' -> none none '|' expr expr expr

#Pass FixedExpressions SameDelimiter SameTerminator
'&' -> none none '&' expr '*' expr '*' expr '*'

'identifier' -> none none `[a-zA-Z]\w*`
