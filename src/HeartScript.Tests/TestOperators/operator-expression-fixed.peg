identifier -> `[_a-zA-Z]\w*`
expr ->
    [
        ('{'          none none '{' expr ',' expr ',' expr '}'),
        ('['          none none '[' expr expr expr ']'),
        ('?'          none none '?' expr ':' expr ':' expr),
        ('|'          none none '|' expr expr expr),
        ('&'          none none '&' expr '*' expr '*' expr '*'),
        ('identifier' none none identifier),
    ]
