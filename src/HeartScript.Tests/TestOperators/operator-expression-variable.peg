expr ->
    [
        '{'          none none '{' (expr (',' expr)*)? '}'
        '['          none none '[' expr* ']'
        '?'          none none '?' ((expr ':')* expr)?
        '|'          none none '|' expr*
        '&'          none none '&' (expr ('*' expr)*)? '*'
        'identifier' none none `[_a-zA-Z]\w*`
    ]

