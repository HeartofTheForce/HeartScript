identifier -> `[_a-zA-Z]\w*`
expr ->
    [
        ('{'          none none '{' (expr (',' expr)*)? '}'),
        ('['          none none '[' expr* ']'),
        ('?'          none none '?' ((expr ':')* expr)?),
        ('|'          none none '|' expr*),
        ('&'          none none '&' (expr ('*' expr)*)? '*'),
        ('identifier' none none identifier),
    ]

